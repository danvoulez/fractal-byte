{
  "$id": "https://ubl.foundation/schemas/receipt.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "UBL Receipt",
  "type": "object",
  "required": ["t","parents","body","body_cid","sig","kid"],
  "properties": {
    "t": { "type":"string", "enum": ["ubl/wa","ubl/transition","ubl/wf","ubl/attestation"] },
    "parents": { "type":"array", "items":{"type":"string"} },
    "body": { "type":"object" },
    "body_cid": { "type":"string" },
    "sig": { "type":"string" },
    "kid": { "type":"string" },
    "observability": { "type":"object" }
  }
}
